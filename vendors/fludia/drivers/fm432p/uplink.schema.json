          "items": {
            "oneOf": [
              { "type": "number" },
              {
                "type": "array",
                "items": [
                  { "type": "string", "format": "date-time" },
                  { "type": "number" }
                ],
                "minItems": 2,
                "maxItems": 2
              }
            ]
          }
        },
        "indexes": {
          "description": "Array containing the indexes. If recvTime is provided, it becomes a 2-dimensional array with JavaScript Date objects: [[2024-08-02T19:30:00.000Z, 1], ...].",
          "type": "array",
          "items": {
            "oneOf": [
              { "type": "number" },
              {
                "type": "array",
                "items": [
                  { "type": "string", "format": "date-time" },
                  { "type": "number" }
                ],
                "minItems": 2,
                "maxItems": 2
              }
            ]
          }
        },
        "meter_type": {
          "type": ["string", "null"],
